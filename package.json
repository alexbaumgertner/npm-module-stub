{
  "name": "npm-package-stub",
  "version": "0.0.0",
  "description": "npm-module-stub",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbaumgertner/npm-module-stub.git"
  },
  "keywords": [
    "npm",
    "stub",
    "module"
  ],
  "author": "alexbaumgertner",
  "contributors": [
    {
      "name": "alexbaumgertner",
      "email": "alex.baumgertner@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexbaumgertner/npm-module-stub/issues"
  },
  "private": true,
  "homepage": "https://github.com/alexbaumgertner/npm-module-stub#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015-loose": "^6.1.3",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "git-hooks": "^1.0.2",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "sinon": "^1.17.2",
    "webpack": "^1.12.9",
    "webpack-notifier": "^1.2.1"
  },
  "options": {
    "mocha": "--require babel-core/register src/**/__tests__/**/*.js",
    "mocha_courses": "--require babel-core/register tests/courses/**/*.js"
  },
  "babel": {
    "ignore": [
      "test/**/*.js"
    ],
    "presets": [
      "es2015-loose"
    ]
  },
  "scripts": {
    "compile": "npm run clean && set NODE_ENV=production && webpack --config ./config/webpack/webpack.production.config.js --progress --profile --colors",
    "compile-dev": "npm run clean && set NODE_ENV=development && webpack --config ./config/webpack/webpack.development.config.js --progress --profile --colors",
    "clean": "rm -rf lib",
    "prepublish": "npm run compile",
    "test": "npm run test:server && npm run test:browser",
    "test:server": "mocha --opts config/mocha.opts",
    "test:browser": "./node_modules/.bin/karma start --single-run",
    "karma:firefox": "./node_modules/.bin/karma start --browsers=Firefox",
    "karma:chrome": "./node_modules/.bin/karma start --browsers=Chrome",
    "eslint:source": "eslint ./src/**/*.js",
    "eslint:fix": "eslint --fix ./src/**/*.js",
    "eslint:common": "eslint ./test/**/*.common.js",
    "eslint:server": "eslint ./test/**/*.server.js",
    "eslint:browser": "eslint ./test/**/*.browser.js"
  },
  "dependencies": {}
}
